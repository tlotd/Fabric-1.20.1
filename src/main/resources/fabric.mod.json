{
	"schemaVersion": 1,
	"id": "tlotd",
	"version": "${version}",
	"name": "TLOTD",
	"description": "The 1.20.1 Fabric Port - This Mod adds new Dimensions and progression after the normal Diamond tier. Additionally this mod provides feats in Engineering and Technology!",
	"authors": [
		"TLOTD",
		"Isla_Nublar"
	],
	"contact": {
		"homepage": "https://tlotd.net/",
		"sources": "https://github.com/Isla-Nublar/tlotd-fabric-1.20.1"
	},
	"license": "CC-BY-NC-SA-4.0",
	"icon": "assets/tlotd/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.tlotd.TLOTD"
		],
		"client": [
			"net.tlotd.TLOTDClient"
		],
		"fabric-datagen": [
			"net.tlotd.TLOTDDataGenerator"
		],
		"rei_client": [
			"net.tlotd.compat.TLOTDREIClientPlugin"
		],
		"modmenu": [
			"net.tlotd.compat.modmenu.TLOTDModMenuIntegration"
		]
